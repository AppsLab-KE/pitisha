{
    "name": "appslabke/pitisha",
    "description": "Pitisha package help you add approval process in your application with multi-level approvals",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "appslab",
        "appslab ke",
        "Pitisha",
        "laravel approval"
    ],
    "support": {
        "issues": "https://github.com/AppsLab-KE/pitisha/issues"
    },
    "homepage": "https://appslab.co.ke",
    "authors": [
        {
            "name": "Marvin Collins",
            "email": "marvin@appslab.co.ke",
            "role": "Senior Software Engineer",
            "homepage": "https://marvinhosea.pro"
        }
    ],
    "minimum-stability": "beta",
    "require": {
        "php": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.1",
        "mockery/mockery": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Pitisha\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pitisha\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pitisha\\ApprovableServiceProvider"
            ]
        }
    }
}
